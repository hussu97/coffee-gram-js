<% include partials/header %>
<div class="container">
  <div class="jumbotron">
    <div class="container">
      <h1>Welcome to TentOut</h1>
      <p>The best site to check out one-of-a-kind camp sites</p>
      <a class="btn btn-primary btn-large" href="/campgrounds/new">Add new Camp Site</a>
    </div>
  </div>
  <div class="row">
    <% campgrounds.forEach((campground)=>{%>
      <div class="col-md-3 col-sm-6">
          <img src="<%= campground.image %>" class="img-thumbnail">
          <h4><%= campground.name %></h4>
      </div>
    <%});%>

  </div>

</div>
<% include partials/footer %>
